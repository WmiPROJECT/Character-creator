package create.characters;

import static org.junit.Assert.*;
import Orc_Warrior;

import org.junit.Test;

public class Back_PanelTest {

	@Test
	public void ChangeNicknameTest(String change_name) {
		Orc_Warrior orc_warrior = new Orc_Warrior("Darek","Mace","Crystalline","Eternal Vitality");
		String name="Darek";
		orc_warrior.ChangeNickname("Marek");
	     assertEquals(orc_warrior.nickname, name);
		
	}

}
