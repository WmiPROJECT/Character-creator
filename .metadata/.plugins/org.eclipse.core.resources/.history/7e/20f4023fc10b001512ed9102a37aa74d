package konwerter;
import java.util.*;
import java.io.*;

public class Converter {
	

	public static void main(String[] args) {
		
		System.out.println("            CLASS CONVERTER      ");
		System.out.println("[===================================]");
		System.out.println("[=== 1. Java to C++              ===]");
		System.out.println("[=== 2. C++ to Java              ===]");
		System.out.println("[===================================]");

	}
}